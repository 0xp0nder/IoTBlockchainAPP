{
  "name": "BlockchainIoT",
  "version": "1.0.0",
  "client": {
    "organization": "Device",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "Device": {
      "mspid": "DeviceMSP",
      "peers": ["peer0.device.networkiot.com", "peer1.device.networkiot.com"],
      "certificateAuthorities": ["ca.device.networkiot.com"]
    },
    "Linkage": {
      "mspid": "LinkageMSP",
      "peers": ["peer0.linkage.networkiot.com", "peer1.linkage.networkiot.com"],
      "certificateAuthorities": ["ca.linkage.networkiot.com"]
    }
  },
  "peers": {
    "peer0.device.networkiot.com": {
      "url": "grpcs://localhost:7051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/device.networkiot.com/tlsca/tlsca.device.networkiot.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.device.networkiot.com",
        "hostnameOverride": "peer0.device.networkiot.com"
      }
    },
    "peer1.device.networkiot.com": {
      "url": "grpcs://localhost:8051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/device.networkiot.com/tlsca/tlsca.device.networkiot.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer1.device.networkiot.com",
        "hostnameOverride": "peer1.device.networkiot.com"
      }
    },
    "peer0.linkage.networkiot.com": {
      "url": "grpcs://localhost:9051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/linkage.networkiot.com/tlsca/tlsca.linkage.networkiot.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.linkage.networkiot.com",
        "hostnameOverride": "peer0.linkage.networkiot.com"
      }
    },
    "peer1.linkage.networkiot.com": {
      "url": "grpcs://localhost:10051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/linkage.networkiot.com/tlsca/tlsca.linkage.networkiot.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer1.linkage.networkiot.com",
        "hostnameOverride": "peer1.linkage.networkiot.com"
      }
    }
  },
  "certificateAuthorities": {
    "ca.device.networkiot.com": {
      "url": "https://localhost:7054",
      "caName": "caDevice",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/device.networkiot.com/tlsca/tlsca.device.networkiot.com-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    },
    "ca.linkage.networkiot.com": {
      "url": "https://localhost:8054",
      "caName": "caLinkage",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/linkage.networkiot.com/tlsca/tlsca.linkage.networkiot.com-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
